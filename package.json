{
  "name": "npm-link-extra",
  "version": "1.2.0",
  "description": "npm or yarn link common modules between a project and a monorepo/multi-packages dir",
  "author": "David Daniel, <david.e.daniel@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "bin": {
    "nlx": "bin/cli.js"
  },
  "scripts": {
    "build": "yarn install --production",
    "bump": "bump --prompt --commit --push",
    "postbump": "editme -w",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DavideDaniel/npm-link-all"
  },
  "keywords": [
    "npm",
    "yarn",
    "link",
    "unlink",
    "show",
    "javascript",
    "monorepos",
    "node",
    "nodejs"
  ],
  "dependencies": {
    "execa": "^0.8.0"
  },
  "devDependencies": {
    "editme": "1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-json": "^2.0.0",
    "eslint-plugin-import": "^2.6.0",
    "version-bump-prompt": "^3.1.0"
  }
}
